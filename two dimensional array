import java.util.Scanner;
public class TwoDEx {

	public static void main(String[] args) {
		int twoD [] [] = new int[3] [3];
		Scanner in = new Scanner(System.in);
		System.out.println("enter elements in 3*3 matrix: ");
		
		for (int i= 0; i<3; i++)
		{
			twoD[i][j]= in.nextInt();
			
		}
	}
          for(int i=0; i<3; i++)
          {
        	  for (int j=0; j<3; j++)
        	  {
        		  System.out.println(twoD[i][j]);
        	  }
          }
}
}
