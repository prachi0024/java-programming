import java.util.*;
public class LoopArrayEx {

	public static void main(String[] args) {
		int n;
		Scanner in = new Scanner(System.in);
		System.out.println("Enter Number of Element : ");
		n = in.nextInt();
		int arr[] = new int [n];
		System.out.println("enter" +n+" elements: ");
		for(int i=0; i<n; i++)
		{
			arr[i] = in.nextInt();
			sum = sum + arr[i];
			
		}
		System.out.println(sum);

	}

}
